ðŸ‘‰ T.C O(N)
    S.C O(1)

class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int mini=prices[0];
        int price=0;
        for(int i=1;i<prices.size();i++){
            int cost=prices[i]-mini;
            price=max(price,cost);
            mini=min(mini,prices[i]);
        }   
        return price;
    }
};
